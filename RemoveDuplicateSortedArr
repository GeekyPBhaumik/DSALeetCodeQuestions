//https://leetcode.com/problems/remove-duplicates-from-sorted-array/description/?envType=study-plan-v2&envId=top-interview-150
class Solution {
    public int removeDuplicates(int[] arr) {
          int i=0;
        for (int j=1; j< arr.length; j++){
            if (arr[i]!= arr[j]){
                i++;
                arr[i]=arr[j];
            }

        }
        return i+1;
    }
    }
      
